Object.defineProperty(exports, "downloadCSVFromMatrix", {
    enumerable: true,
    get: function() {
        return downloadCSVFromMatrix;
    }
});
function downloadCSVFromMatrix({ headers, rows, filename = "data.csv" }) {
    const escape = (value)=>`"${String(value !== null && value !== void 0 ? value : "").replace(/"/g, '""')}"`;
    const csvContent = [
        headers,
        ...rows
    ].map((row)=>row.map(escape).join(",")).join("\n");
    const blob = new Blob([
        csvContent
    ], {
        type: "text/csv;charset=utf-8;"
    });
    const url = URL.createObjectURL(blob);
    const link = document.createElement("a");
    link.href = url;
    link.setAttribute("download", filename);
    document.body.appendChild(link);
    link.click();
    document.body.removeChild(link);
    URL.revokeObjectURL(url);
}

