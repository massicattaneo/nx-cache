"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "getUserInitials", {
    enumerable: true,
    get: function() {
        return getUserInitials;
    }
});
const getUserInitials = (fullName)=>{
    if (!fullName || !fullName.trim()) {
        return "?";
    }
    return fullName.trim().split(" ").map((name)=>name.trim().charAt(0).toUpperCase()).filter((letters)=>letters).slice(0, 2).join("");
};

