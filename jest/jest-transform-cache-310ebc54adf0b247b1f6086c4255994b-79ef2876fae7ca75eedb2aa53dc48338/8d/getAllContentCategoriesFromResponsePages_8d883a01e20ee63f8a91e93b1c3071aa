// We have to build and add the "all"
const allContentCategory = {
    id: "all",
    name: "All sources",
    logo: "FilterListOutlined",
    sourceCount: null
};
function getAllContentCategoriesFromResponsePages({ pages }) {
    const contentCategories = (pages === null || pages === void 0 ? void 0 : pages.flatMap((page)=>page.results)) || [];
    const contentCategoriesWithSources = contentCategories.filter((category)=>category.sourceCount);
    return [
        allContentCategory,
        ...contentCategoriesWithSources
    ];
}

