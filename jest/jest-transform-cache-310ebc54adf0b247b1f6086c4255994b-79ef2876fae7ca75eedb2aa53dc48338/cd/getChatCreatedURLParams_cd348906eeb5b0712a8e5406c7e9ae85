function getChatCreatedURLParams({ chatId, hashParams, locationSearch }) {
    const searchParams = new URLSearchParams(locationSearch);
    if (chatId) {
        searchParams.set("chatId", chatId);
    } else {
        searchParams.delete("chatId");
    }
    const hashParamsListString = Object.entries(hashParams).reduce((acc, [key, value])=>{
        const newAcc = acc + `${acc ? "&" : ""}${key}=${value}`;
        return newAcc;
    }, "");
    const hashParamsURLString = hashParamsListString ? `#?${hashParamsListString}` : "";
    const urlParams = `?${searchParams.toString()}${hashParamsURLString}`;
    return urlParams;
}

