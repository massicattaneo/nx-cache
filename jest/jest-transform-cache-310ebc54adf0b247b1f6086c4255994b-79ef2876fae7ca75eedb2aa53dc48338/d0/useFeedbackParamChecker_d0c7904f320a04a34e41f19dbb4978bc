const _react = require("react");
const _reactrouterdom = require("react-router-dom");
const useFeedbackUrlParamChecker = ({ isFeedbackMode, setIsFeedbackMode })=>{
    const [searchParams, setSearchParams] = (0, _reactrouterdom.useSearchParams)();
    (0, _react.useEffect)(()=>{
        const feedbackParam = searchParams.get("feedback");
        if (feedbackParam) {
            // /?feedback=true
            if (feedbackParam === "true" && !isFeedbackMode) {
                setIsFeedbackMode(true);
            }
            // /?feedback=false
            if (feedbackParam === "false" && isFeedbackMode) {
                setIsFeedbackMode(false);
            }
            // Remove the param
            setSearchParams();
        }
    }, [
        isFeedbackMode,
        searchParams,
        setIsFeedbackMode,
        setSearchParams
    ]);
};

