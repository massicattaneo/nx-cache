Object.defineProperty(exports, "generateMacroIds", {
    enumerable: true,
    get: function() {
        return generateMacroIds;
    }
});
const generateMacroIds = (macroFilter)=>{
    const ids = [];
    const baseId = macroFilter.type;
    for (const group of macroFilter.value){
        const groupId = group.type;
        for (const key of group.value){
            ids.push(`${baseId}-${groupId}-${key}`);
        }
    }
    return ids;
};

