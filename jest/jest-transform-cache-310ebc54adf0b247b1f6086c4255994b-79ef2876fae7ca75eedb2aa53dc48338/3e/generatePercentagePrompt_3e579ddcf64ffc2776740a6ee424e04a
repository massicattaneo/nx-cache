Object.defineProperty(exports, "generatePercentagePrompt", {
    enumerable: true,
    get: function() {
        return generatePercentagePrompt;
    }
});
function generatePercentagePrompt(name, verb, percentage, end) {
    const upDown = percentage === 0 ? "unchanged" : percentage > 0 ? "up" : "down";
    const displayedMessage = `Why ${verb} ${name} ${upDown} ${Math.abs(percentage)}% ${end}?`;
    return {
        displayedMessage: percentage !== 0 ? displayedMessage : undefined,
        prompt: `Why ${verb} ${name} ${upDown} ${end}?`
    };
}

