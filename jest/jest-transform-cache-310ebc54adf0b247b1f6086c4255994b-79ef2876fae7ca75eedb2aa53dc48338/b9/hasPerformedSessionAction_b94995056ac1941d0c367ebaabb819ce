function hasPerformedSessionAction(action, currentPath, comparativePath) {
    if (!comparativePath || currentPath !== comparativePath) return false;
    const key = `${action}:${comparativePath}`;
    const alreadySeen = sessionStorage.getItem(key);
    if (alreadySeen) return false;
    sessionStorage.setItem(key, "true");
    return true;
}

