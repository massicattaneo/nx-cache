const _constants = require("../constants");
const selectFindsByIndex = (findIndex)=>`${_constants.CLASS_NAMES.TYPE_PREFIX}${_constants.TYPES.FIND}-${findIndex}`;
const selectFindByCount = (findIndex, findCount)=>`${_constants.CLASS_NAMES.ID_PREFIX}${_constants.CLASS_NAMES.FIND_PREFIX}${findIndex}-${findCount}`;
function getFindMatchesLength(element) {
    if (!element) return 0;
    const array = Array.from(element.querySelectorAll(`.${selectFindsByIndex(0)}`));
    const regExp = new RegExp(`${selectFindByCount(0, "(\\d*)")}`);
    const matches = array.map((element)=>element.className).map((className)=>className.match(regExp)).map((match)=>match ? Number(match[1]) : -1);
    return matches.length ? Math.max(...matches) + 1 : 0;
}

