function getXUrl(itemUrl, message) {
    const searchParams = new URLSearchParams({
        text: message,
        url: itemUrl
    });
    return `https://x.com/intent/tweet?${searchParams.toString()}`;
}
function getFacebookUrl(itemUrl) {
    const searchParams = new URLSearchParams({
        u: itemUrl
    });
    return `https://www.facebook.com/sharer/sharer.php?${searchParams.toString()}`;
}
function getRedditUrl(itemUrl, title) {
    const searchParams = new URLSearchParams({
        url: itemUrl,
        title
    });
    return `http://www.reddit.com/submit?${searchParams.toString()}`;
}
function getEmailUrl(subject, body) {
    return `mailto:?subject=${subject}&body=${body}`;
}

