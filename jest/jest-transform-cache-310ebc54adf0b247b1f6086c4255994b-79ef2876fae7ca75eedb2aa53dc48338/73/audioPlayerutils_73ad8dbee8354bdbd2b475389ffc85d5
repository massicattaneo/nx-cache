const convertSecondsToTime = (time)=>{
    if (time < 0 || isNaN(time)) return "";
    const hours = Math.floor(time / 3600);
    const timeWithoutHours = time % 3600;
    const minutes = Math.floor(timeWithoutHours / 60);
    const seconds = Math.floor(timeWithoutHours % 60);
    if (hours) return `${hours}:${minutes.toString().padStart(2, "0")}:${seconds.toString().padStart(2, "0")}`;
    if (minutes) return `${minutes}:${seconds.toString().padStart(2, "0")}`;
    return `0:${seconds.toString().padStart(2, "0")}`;
};
const normalizeCurrentTimeChange = (newValue)=>Array.isArray(newValue) ? newValue[0] : newValue;
const initializeAudioPlayer = (url, audio, setAudio)=>{
    if (!url || audio) return;
    const newAudio = new Audio(url);
    setAudio(newAudio);
};
const createOnTimeUpdateAudioPlayer = (audio, setCurrentTime, setDuration, duration)=>()=>{
        if (!audio) return undefined;
        setCurrentTime(Math.round(audio.currentTime));
        if (!duration) {
            setDuration(Math.round(audio.duration));
        }
    };
const playAudioPlayer = (audio, setIsPlaying, setAreControlsVisible)=>{
    if (!audio) return;
    audio.play();
    setIsPlaying(true);
    setAreControlsVisible(true);
};
const pauseAudioPlayer = (audio, setIsPlaying)=>{
    if (!audio) return;
    audio.pause();
    setIsPlaying(false);
};
const closeAudioPlayerControls = (audio, setIsPlaying, setAreControlsVisible)=>{
    if (!audio) return;
    audio.pause();
    setIsPlaying(false);
    setAreControlsVisible(false);
};

