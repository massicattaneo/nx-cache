function getAutosuggestSourceRowStatus({ isSource, sourceSelectionPackages, isSourceSelected, isPackageSelected }) {
    const isSelectedAsItself = !!(isSource ? isSourceSelected : isPackageSelected);
    const hasPackageIntersection = !!sourceSelectionPackages;
    const isSelected = hasPackageIntersection || isSelectedAsItself;
    const isDisabled = isSource && hasPackageIntersection;
    const tooltipText = sourceSelectionPackages ? `Selected in ${(sourceSelectionPackages === null || sourceSelectionPackages === void 0 ? void 0 : sourceSelectionPackages.length) === 1 ? "package" : "packages"}: ${sourceSelectionPackages === null || sourceSelectionPackages === void 0 ? void 0 : sourceSelectionPackages.join(", ")}` : null;
    return {
        isDisabled,
        isThisCheckboxChecked: isSelected,
        tooltipText
    };
}

