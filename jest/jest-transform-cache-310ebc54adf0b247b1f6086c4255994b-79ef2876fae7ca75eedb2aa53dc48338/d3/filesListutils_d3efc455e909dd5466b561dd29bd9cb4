const handleCheckboxChangeFunc = ({ ev, val, setSelectedFiles, completedFiltered, shared })=>{
    ev.stopPropagation();
    if (val) {
        setSelectedFiles(completedFiltered.map((f)=>({
                id: f.id,
                shared
            })));
    } else {
        setSelectedFiles([]);
    }
};
const getOwnerNameData = (user, orgUser)=>{
    if (!orgUser || !user) return {
        ownerName: undefined,
        ownerDisplayName: undefined
    };
    const ownerName = `${orgUser.publicUserData.firstName} ${orgUser.publicUserData.lastName}`;
    const ownerDisplayName = orgUser.publicUserData.userId === user.id ? "You" : ownerName;
    return {
        ownerName,
        ownerDisplayName
    };
};

