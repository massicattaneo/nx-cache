Object.defineProperty(exports, "sortBreakpoints", {
    enumerable: true,
    get: function() {
        return sortBreakpoints;
    }
});
const sortBreakpoints = (first, second)=>{
    const byIndex = first.index - second.index;
    if (byIndex !== 0) return byIndex;
    if (first.id === "start" || second.id === "end") return -1;
    if (second.id === "start" || first.id === "end") return 1;
    if (first.isStart && (second.isEnd || second.isSentenceNumber)) return 1;
    if (first.isEnd && (second.isStart || second.isSentenceNumber)) return -1;
    if (first.isSentenceNumber && second.isEnd) return 1;
    if (first.isSentenceNumber && second.isStart) return -1;
    return 0;
};

