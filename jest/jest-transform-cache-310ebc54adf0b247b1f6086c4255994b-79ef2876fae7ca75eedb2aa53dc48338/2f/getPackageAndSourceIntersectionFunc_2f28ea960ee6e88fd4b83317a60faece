function getPackageAndSourceIntersectionFunc(autosuggestSource, selectedAllPackages) {
    const matchedPackages = selectedAllPackages.filter((contentPackage)=>autosuggestSource.rpPackageId.includes(contentPackage.id));
    const matchedPackageNames = matchedPackages.map((contentPackage)=>contentPackage.name);
    return matchedPackageNames.length > 0 ? matchedPackageNames : null;
}

