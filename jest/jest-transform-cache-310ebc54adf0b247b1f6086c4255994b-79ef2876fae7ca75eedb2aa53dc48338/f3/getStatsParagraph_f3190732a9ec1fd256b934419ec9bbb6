Object.defineProperty(exports, "getStatsParagraph", {
    enumerable: true,
    get: function() {
        return getStatsParagraph;
    }
});
function getStatsParagraph(stats, paragraphNumber, sentenceNumber) {
    if (!stats.paragraphs[paragraphNumber]) return undefined;
    const p = stats.paragraphs[paragraphNumber].find((par)=>par.sentences.find((s)=>s.sentenceNumber === sentenceNumber));
    const content = p !== null && p !== void 0 ? p : stats.paragraphs[paragraphNumber][0];
    return content;
}

