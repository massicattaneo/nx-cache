"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "filterAndSortWatchlists", {
    enumerable: true,
    get: function() {
        return filterAndSortWatchlists;
    }
});
const filterAndSortWatchlists = (watchlists, nameFilter)=>{
    return watchlists// Filter wathlists by name
    .filter((watchlist)=>nameFilter !== undefined ? watchlist.name.toLowerCase().includes(nameFilter.toLowerCase()) : true)// Sort watchilsts from newest to oldest
    .sort((firstWatchlist, secondWatchlist)=>{
        if (firstWatchlist.lastUpdated < secondWatchlist.lastUpdated) {
            return 1;
        }
        if (firstWatchlist.lastUpdated > secondWatchlist.lastUpdated) {
            return -1;
        }
        return 0;
    });
};

