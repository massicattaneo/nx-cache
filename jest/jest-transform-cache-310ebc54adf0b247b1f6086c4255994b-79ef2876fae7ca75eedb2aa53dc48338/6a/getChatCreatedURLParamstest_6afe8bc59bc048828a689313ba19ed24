"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _getChatCreatedURLParams = require("./getChatCreatedURLParams");
describe("getChatCreatedURLParams", ()=>{
    describe("with new chat id", ()=>{
        describe("with no hash params", ()=>{
            const chatId = "123";
            const hashParams = {};
            const locationSearch = "chatId=123";
            it("should return correct string", ()=>{
                expect((0, _getChatCreatedURLParams.getChatCreatedURLParams)({
                    chatId,
                    hashParams,
                    locationSearch
                })).toBe("?chatId=123");
            });
        });
        describe("with 1 hash param", ()=>{
            const chatId = "123";
            const hashParams = {
                private: "true"
            };
            const locationSearch = "chatId=123";
            it("should return correct string", ()=>{
                expect((0, _getChatCreatedURLParams.getChatCreatedURLParams)({
                    chatId,
                    hashParams,
                    locationSearch
                })).toBe("?chatId=123#?private=true");
            });
        });
        describe("with multiple hash params", ()=>{
            const chatId = "123";
            const hashParams = {
                private: "true",
                foo: "123",
                bar: "baz"
            };
            const locationSearch = "chatId=123";
            it("should return correct string", ()=>{
                expect((0, _getChatCreatedURLParams.getChatCreatedURLParams)({
                    chatId,
                    hashParams,
                    locationSearch
                })).toBe("?chatId=123#?private=true&foo=123&bar=baz");
            });
        });
    });
    describe("with empty chat id overwriting existing chat id", ()=>{
        const chatId = undefined;
        const hashParams = {};
        const locationSearch = "chatId=123";
        it("should return correct string", ()=>{
            expect((0, _getChatCreatedURLParams.getChatCreatedURLParams)({
                chatId,
                hashParams,
                locationSearch
            })).toBe("?");
        });
    });
});

