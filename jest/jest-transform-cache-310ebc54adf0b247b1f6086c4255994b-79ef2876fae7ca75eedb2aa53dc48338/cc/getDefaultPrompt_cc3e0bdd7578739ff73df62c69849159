"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "getDefaultPrompt", {
    enumerable: true,
    get: function() {
        return getDefaultPrompt;
    }
});
const _chat = require("@rp/react/common/chat");
const DEFAULT_SUMMARIZE_PROMPT = "Summarize the key insights in this document";
const DEFAULT_SUMMARIZE_PROMPT_PLURAL = "Summarize the key insights in these documents";
const DEFAULT_WATCHLIST_PROMPT = "Give me the latest news";
const DEFAULT_WATCHLIST_AND_FILES_PROMPT = "Give me insights about my files";
const getDefaultPrompt = (fileIds, watchlistId, dataset)=>{
    if (fileIds !== undefined && fileIds.length === 1) {
        return DEFAULT_SUMMARIZE_PROMPT;
    }
    if (fileIds !== undefined && fileIds.length > 1) {
        return DEFAULT_SUMMARIZE_PROMPT_PLURAL;
    }
    if (watchlistId !== undefined && (dataset === undefined || dataset === "news")) {
        return DEFAULT_WATCHLIST_PROMPT;
    }
    if (watchlistId !== undefined && dataset === "files") {
        return DEFAULT_WATCHLIST_AND_FILES_PROMPT;
    }
    if (watchlistId !== undefined && dataset !== undefined) {
        return `Give me the latest ${_chat.chatDatasets[dataset].label} news`;
    }
    return "Error";
};

