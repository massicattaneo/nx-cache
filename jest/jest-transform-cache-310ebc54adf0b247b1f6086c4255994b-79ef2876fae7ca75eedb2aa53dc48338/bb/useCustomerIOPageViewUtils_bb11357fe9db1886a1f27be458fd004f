const extractRouteParams = (pathname, search)=>{
    const searchParams = new URLSearchParams(search);
    const isNewWatchlistPath = (0, _reactrouterdom.matchPath)(_router.WATCHLIST_NEW, pathname);
    const isWatchlist = !isNewWatchlistPath && (0, _reactrouterdom.matchPath)(_router.WATCHLIST_ID_ROUTE, pathname);
