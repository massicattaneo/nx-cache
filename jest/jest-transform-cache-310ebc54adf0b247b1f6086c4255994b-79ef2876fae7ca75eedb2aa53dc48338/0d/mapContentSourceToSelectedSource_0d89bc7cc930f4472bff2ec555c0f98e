function mapContentSourceToSelectedSource(contentSourceResponse) {
    if (!contentSourceResponse.data) {
        return {
            id: "",
            name: "",
            packageIDs: []
        };
    }
    return {
        id: contentSourceResponse.data.id,
        name: contentSourceResponse.data.name,
        packageIDs: contentSourceResponse.data.packageIds || []
    };
}

