function mapContentPackageToSelectedPackage(contentPackageResponse) {
    if (!contentPackageResponse.data) {
        return {
            id: "",
            name: "",
            sourceCount: 0
        };
    }
    return {
        id: contentPackageResponse.data.id,
        name: contentPackageResponse.data.name,
        sourceCount: contentPackageResponse.data.sourceCount
    };
}

