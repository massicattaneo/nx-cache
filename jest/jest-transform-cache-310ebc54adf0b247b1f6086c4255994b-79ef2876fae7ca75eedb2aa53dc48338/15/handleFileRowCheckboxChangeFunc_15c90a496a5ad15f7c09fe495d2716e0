"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "handleFileRowCheckboxChangeFunc", {
    enumerable: true,
    get: function() {
        return handleFileRowCheckboxChangeFunc;
    }
});
function handleFileRowCheckboxChangeFunc({ actions, checked, isSelected, isThisFileOpen, item, openChatPane, setSelectedFiles, shared }) {
    if (isSelected && isThisFileOpen) {
        actions.close.onClick();
    }
    if (!isSelected) {
        actions.open.onClick(item);
        openChatPane();
    }
    setSelectedFiles((prev)=>{
        return checked ? [
            ...prev,
            {
                id: item.id,
                shared: !!shared
            }
        ] : prev.filter((i)=>i.id !== item.id);
    });
}

