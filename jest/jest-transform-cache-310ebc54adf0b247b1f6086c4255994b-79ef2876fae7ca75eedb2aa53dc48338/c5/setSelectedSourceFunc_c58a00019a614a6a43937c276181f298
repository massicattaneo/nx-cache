"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "setSelectedSourceFunc", {
    enumerable: true,
    get: function() {
        return setSelectedSourceFunc;
    }
});
function setSelectedSourceFunc({ source, selectedSources, setSelectedSources, planComparisonHandleOpen }) {
    if (!source.entitled) {
        planComparisonHandleOpen();
        return;
    }
    const idx = selectedSources.findIndex((s)=>s.id === source.id);
    if (idx === -1) {
        const newSource = {
            id: source.id,
            name: source.name,
            packageIDs: source.rpPackageId
        };
        setSelectedSources((prev)=>[
                newSource,
                ...prev
            ]);
    } else {
        setSelectedSources((prev)=>[
                ...prev.slice(0, idx),
                ...prev.slice(idx + 1)
            ]);
    }
}

