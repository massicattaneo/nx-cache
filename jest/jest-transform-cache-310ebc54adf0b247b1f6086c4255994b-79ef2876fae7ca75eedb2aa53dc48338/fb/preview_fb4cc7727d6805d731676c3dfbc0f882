"use strict";
const fs = require("fs");
const { CACHE_DIRECTORY, INDEX_PATH } = require("./server");
const preview = ()=>{
    if (!fs.existsSync(CACHE_DIRECTORY)) {
        fs.mkdirSync(CACHE_DIRECTORY, {
            recursive: true
        });
    }
    fs.writeFileSync(INDEX_PATH, document.documentElement.outerHTML);
};
module.exports = {
    preview
};

