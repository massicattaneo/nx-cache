const limiter = (primaryColor)=>Math.min(200, Math.max(50, primaryColor));
const limitColor = (color)=>{
    const number = parseInt(color, 16);
    const red = limiter(number >> 16);
    const blue = limiter(number >> 8 & 0x00ff);
    const green = limiter(number & 0x0000ff);
    return (green | blue << 8 | red << 16).toString(16);
};
const entityIdToColor = (entityId = "", opacity = 0.4)=>{
    return `#${limitColor(entityId)}${(opacity * 100).toFixed(0)}`;
};
const eventIdToColor = (eventId = "")=>{
    let hash = 0;
    for(let i = 0; i < eventId.length; i++){
        hash = eventId.charCodeAt(i) + ((hash << 5) - hash);
    }
    let colour = "";
    for(let i = 0; i < 3; i++){
        const value = hash >> i * 8 & 0xff;
        colour += ("00" + value.toString(16)).slice(-2);
    }
    return `#${limitColor(colour)}`;
};

