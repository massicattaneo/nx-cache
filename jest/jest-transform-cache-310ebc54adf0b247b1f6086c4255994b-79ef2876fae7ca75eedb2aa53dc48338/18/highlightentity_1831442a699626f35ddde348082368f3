const _constants = require("../constants");
const highlightEntity = (style = {}, { id })=>{
    const type = id.indexOf(",") !== -1 ? _constants.TYPES.EVENT : _constants.TYPES.ENTITY;
    const mapped = id.split(",").filter((item)=>item).map((subItem)=>`.${_constants.CLASS_NAMES.ID_PREFIX}${subItem}`).join("");
    return {
        style,
        className: `& .${_constants.CLASS_NAMES.TYPE_PREFIX}${type}${mapped}`
    };
};

