"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "getUrlWithTextFragment", {
    enumerable: true,
    get: function() {
        return getUrlWithTextFragment;
    }
});
const AMOUNT_START = 5;
const AMOUNT_END = 2;
// We want to get the first 3 and last 3 words of a sentence, to use them as prefix and suffix.
const getUrlWithTextFragment = (url, textFragment)=>{
    const encodedTextFragment = encodeURIComponent(textFragment);
    const encodedTextFragmentHyphens = encodedTextFragment.replace(/-/g, "%2D");
    const words = encodedTextFragmentHyphens.split("%20");
    return words.length <= AMOUNT_START + AMOUNT_END ? `${url}#:~:text=${encodedTextFragmentHyphens}` : `${url}#:~:text=${words.slice(0, AMOUNT_START).join("%20")},${words.slice(-AMOUNT_END).join("%20")}`;
};

