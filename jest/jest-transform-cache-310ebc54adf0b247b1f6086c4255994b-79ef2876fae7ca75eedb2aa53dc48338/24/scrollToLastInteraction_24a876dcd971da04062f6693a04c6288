const _Chat = require("./PersistedChat/Chat");
const CHAT_SCROLL_TOP_OFFSET = 76;
const scrollToLastInteraction = ()=>{
    const all = window.document.querySelectorAll(`.${_Chat.INTERACTION_CLASS_NAME}`);
    const last = all[all.length - 1];
    const interactionContainer = last === null || last === void 0 ? void 0 : last.getBoundingClientRect();
    interactionContainer && window.scroll({
        top: window.scrollY + interactionContainer.y - CHAT_SCROLL_TOP_OFFSET,
        behavior: "smooth"
    });
};

