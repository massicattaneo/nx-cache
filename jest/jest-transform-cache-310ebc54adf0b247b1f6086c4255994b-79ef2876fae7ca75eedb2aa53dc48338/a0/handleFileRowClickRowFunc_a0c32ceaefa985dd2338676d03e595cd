function handleFileRowClickRowFunc({ actions, item, isSelected, isThisFileOpen, openChatPane, setSelectedFiles, shared }) {
    if (!isSelected) {
        setSelectedFiles((prev)=>[
                {
                    id: item.id,
                    shared: !!shared
                },
                ...prev
            ]);
        actions.open.onClick(item);
        return;
    }
    if (isThisFileOpen) {
        actions.close.onClick();
        return;
    }
    openChatPane();
    actions.open.onClick(item);
}

