const _routeConstants = require("./routeConstants");
const getUpgradePlanUrl = (currentUser, product, isEnterprise, subscriptionFrequency)=>{
    if (isEnterprise) {
        return _routeConstants.CONTACT_SALES_ROUTE;
    }
    return `${currentUser === "free" ? _routeConstants.UPGRADE_ROUTE : _routeConstants.ADD_CONTENT_ROUTE}${product ? `?product=${product}&frequency=${subscriptionFrequency ? subscriptionFrequency.toLocaleLowerCase() : "yearly"}` : ""}`;
};
const redirectToUpdatePlan = (navigate, currentUser, product, isEnterprise, subscriptionFrequency, options)=>{
    navigate(getUpgradePlanUrl(currentUser, product, isEnterprise, subscriptionFrequency), options);
};

